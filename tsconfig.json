{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES2015", "DOM"],
    "declaration": false,
    "outDir": "./dist/",
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "types": ["cypress"],
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src", "index.d.ts"]
}